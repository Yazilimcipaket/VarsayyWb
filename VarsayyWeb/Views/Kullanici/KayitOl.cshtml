
@{
    ViewBag.Title = "KayitOl";
}

<br /><br />
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <section class="panel">
                <header class="panel-heading"></header>
                <div class="panel-body">
                    <form role="form" class="form-horizontal tasi-form" action="/Kullanici/KayitOl" method="post">
                        <div class="form-group has-success">
                            <label class="col-lg-4 ">Adı</label>
                            <div class="col-lg-12">
                                <input type="text" placeholder="Ad" id="l-name" class="form-control" name="Adi">
                            </div>
                        </div>
                        <div class="form-group has-success">
                            <label class="col-lg-4 ">Soyadı</label>
                            <div class="col-lg-12">
                                <input type="text" class="spinner-input form-control" name="Soyadi" placeholder="Soyad">
                            </div>
                        </div>
                        <div class="form-group has-success">
                            <label class="col-lg-4 ">Eposta</label>
                            <div class="col-lg-12">
                                <input type="email" class="spinner-input form-control" name="Eposta" placeholder="Eposta">
                            </div>
                        </div>
                        <div class="form-group has-success">
                            <label class="col-lg-4 ">Parola</label>
                            <div class="col-lg-12">
                                <input type="password" class="spinner-input form-control" name="Parola" placeholder="Parola">
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-lg-offset-2 col-lg-10">
                                <button class="btn btn-danger" type="submit">Kayıt Ol</button>
                            </div>
                        </div>
                    </form>
                </div>
            </section>
        </div>
    </div>
</div>



