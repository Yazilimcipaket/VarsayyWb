
@{
    Layout = null;
}
@model IEnumerable<DtoSiparis>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <link rel="shortcut icon" href="img/favicon.png">

    <title>Varsayy  </title>

    <!-- Bootstrap core CSS -->
    <link href="/Content/Admin/css/bootstrap.min.css" rel="stylesheet">
    <link href="/Content/Admin/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="/Content/Admin/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="/Content/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="/Content/Admin/css/owl.carousel.css" type="text/css">
    <link href="/Content/Admin/css/style.css" rel="stylesheet">
    <link href="/Content/Admin/css/style-responsive.css" rel="stylesheet" />
</head>

<body>

    <section id="container">
        <header class="header white-bg">
            <div class="sidebar-toggle-box">
                <div data-original-title="Toggle Navigation" data-placement="right" class="icon-reorder tooltips"></div>
            </div>
            <a href="/Admin/Index" class="logo">STK <span>Ersağ Otomasyon</span></a>
            <div class="top-nav ">
                <ul class="nav pull-right top-menu">
                    <li>
                        <input type="text" class="form-control search" placeholder="Ara">
                    </li>
                    <li class="dropdown">
                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img alt="" src="/Content/Admin/img/avatar1_small.jpg">
                            <span class="username">Muhammed Ali</span>
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu extended logout">
                            <li><a href="/Admin/CikisYap"><i class="icon-key"></i> Cıkış YAp</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </header>
        <aside>
            <div id="sidebar" class="nav-collapse ">
            </div>
        </aside>
        <section id="main-content">
            <section class="panel">
                <header class="panel-heading">
                    <h3>Urunler</h3>
                </header>
                <div class="panel-body">
                    <div class="adv-table editable-table ">
                        <br />
                        <div class="clearfix">
                            <div class="btn-group">
                            </div>

                        </div>
                        <div class="space15"></div>
                        <table class="table table-striped table-hover table-bordered" id="editable-sample">
                            @foreach (var item in Model)
                            {
                                <thead>
                                    <tr>
                                        <th>Sipariş Tarihi</th>
                                        <th>Kullanıcı</th>
                                        <th>Urunler</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr class="">
                                        <td class="center">@item.SiparisTarihi</td>
                                        <td class="center">@item.kullanici.Adi-@item.kullanici.Soyadi-@item.kullanici.Adres</td>
                                        @foreach (var item2 in item.sepetim)
                                        {
                                            <td class="center">@item2.UrunMarka-@item2.UrunAdi-@item2.UrunFiyat</td>
                                        }
                                    </tr>

                                </tbody>
                            }

                        </table>
                    </div>
                </div>
            </section>
        </section>
        <footer class="site-footer">
            <div class="text-center">
                2019 &copy; Powered by STK
                <a href="#" class="go-top">
                    <i class="icon-angle-up"></i>
                </a>
            </div>
        </footer>
    </section>
    <script src="/Content/Admin/js/jquery.js"></script>
    <script src="/Content/Admin/js/jquery-1.8.3.min.js"></script>
    <script src="/Content/Admin/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="/Content/Admin/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/Content/Admin/js/jquery.scrollTo.min.js"></script>
    <script src="/Content/Admin/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="/Content/Admin/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="/Content/Admin/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="/Content/Admin/js/owl.carousel.js"></script>
    <script src="/Content/Admin/js/jquery.customSelect.min.js"></script>
    <script src="/Content/Admin/js/respond.min.js"></script>
    <script class="include" type="text/javascript" src="/Content/Admin/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="/Content/Admin/js/common-scripts.js"></script>
    <script src="/Content/Admin/js/sparkline-chart.js"></script>
    <script src="/Content/Admin/js/easy-pie-chart.js"></script>
    <script src="/Content/Admin/js/count.js"></script>
    <script>
      $(document).ready(function() {
          $("#owl-demo").owlCarousel({
              navigation : true,
              slideSpeed : 300,
              paginationSpeed : 400,
              singleItem : true,
			  autoPlay:true

          });
      });
      $(function(){
          $('select.styled').customSelect();
      });
    </script>
</body>
</html>
